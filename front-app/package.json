{
    "name": "svelte-routed-app",
    "version": "1.0.0",
    "scripts": {
        "build": "rollup  -c",
        "dev": "rollup  -c -w",
        "start": "sirv public --host 0.0.0.0 --port 2222 --single",
        "firebase": "npm run build && cp ./public/build/bundle.css ./public/build/bundle.js ../firebase/unify-public && cd ../firebase && firebase deploy --only hosting:unify-tn"
    },
    "devDependencies": {
        "@rollup/plugin-commonjs": "^17.0.0",
        "@rollup/plugin-node-resolve": "^11.0.0",
        "rollup": "^2.39.0",
        "rollup-plugin-css-only": "^3.1.0",
        "rollup-plugin-livereload": "^2.0.0",
        "rollup-plugin-svelte": "^7.0.0",
        "rollup-plugin-terser": "^7.0.0",
        "svelte": "^3.32.3"
    },
    "dependencies": {
        "@rollup/plugin-replace": "^2.3.4",
        "@sentry/browser": "^6.2.0",
        "@sentry/tracing": "^6.2.0",
        "animatejs": "^1.0.1",
        "nanoid": "^3.1.20",
        "rollup-plugin-uglify": "^6.0.4",
        "sirv-cli": "^1.0.11",
        "svelte-routing": "^1.5.0",
        "svelte-select": "^3.17.0",
        "svelte-watch-resize": "^1.0.3"
    }
}
