        "rollup-plugin-svelte": "^6.0.0",
        "rollup-plugin-terser": "^7.0.0",
        "svelte": "^3.0.0"
    },
    "dependencies": {
        "@rollup/plugin-replace": "^2.3.4",
        "dom-to-image": "^2.6.0",
        "html2canvas": "^1.0.0-rc.7",
        "nanoid": "^3.1.20",
        "sirv-cli": "^1.0.0",
        "svelte-routing": "^1.4.2"
